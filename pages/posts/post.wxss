swiper{
  width: 100%;
  height: 500rpx;
}

image{
  width: 100%;
  height: 500rpx;
}

.big-Content{
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  padding-bottom: 5px;
}

.icon-Date-Content{
  margin: 10rpx 0 20rpx 10rpx;
}

.iconSS{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}

.dataSS{
  margin-left: 20rpx;
  margin-bottom: 5px;
  vertical-align: middle;
  font-size: 26rpx;
}

.titleSS{
  font-size: 34rpx;
  font-weight: 600;
  color:#333;
  margin-left: 10px;
  margin-bottom: 10px;
}

.bigPicSS{
  margin-left: 16px;
  width: 100%;
  height: 340rpx;
  margin: auto 0;
  margin-bottom: 15px;
}

.desSS{
  color: #666;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  letter-spacing: 2rpx;
  line-height: 40rpx;
}

.nums-content{
  font-size: 13px;
  flex-direction: row;
  line-height: 16px;
  margin-left: 10px;
}

.numIconSS{
  width: 16px;
  height: 16px;
  margin-right: 8px;
  vertical-align: middle;
}

.numTextSS{
  vertical-align: middle;
  margin-right: 20px;
}